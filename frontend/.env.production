# Production Environment Variables for NutriAgent AI Frontend

# API Configuration - This will be set by Render automatically
VITE_API_BASE_URL=https://nutriagent-ai-backend.onrender.com

# Google OAuth - This needs to be configured manually in Render
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# App Configuration
VITE_APP_NAME="NutriAgent AI"
VITE_APP_VERSION="1.0.0"
VITE_APP_ENVIRONMENT="production"
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE_MODE=true